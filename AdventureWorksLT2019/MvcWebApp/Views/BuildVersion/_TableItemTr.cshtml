@using AdventureWorksLT2019.Resx;
@using Framework.Mvc.Models;
@using Framework.Common;
@using AdventureWorksLT2019.Models;
@model MvcItemViewModel<BuildVersionDataModel>
@inject IUIStrings localizor;
@{
    var routeId = Model.Model?.SystemInformationID + "/" + Model.Model?.VersionDate + "/" + Model.Model?.ModifiedDate;
}
<tr class="pt-0 pb-0 nt-listitem@(Model.IsCurrentItem ? " nt-created border-warning border-3": "")" data-nt-route-id="@routeId">
    @await Html.PartialAsync($"~/Views/BuildVersion/_Table{Model.Template}Item.cshtml", Model)
</tr>
